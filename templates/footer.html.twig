<!--==========================
  Contact Section
============================-->
<section id="contact">
    <div class="container wow fadeInUp">
        <div class="section-header">
            <h3 class="section-title">Contact</h3>
            <p class="section-description">Sed ut perspiciatis unde omnis iste natus error sit voluptatem accusantium doloremque</p>
        </div>
    </div>

    <!-- Uncomment below if you wan to use dynamic maps -->
    <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d10501.896943014895!2d2.3897343!3d48.8491666!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x5891d8d62e8535c7!2sESGI%2C+%C3%89cole+Sup%C3%A9rieure+de+G%C3%A9nie+Informatique!5e0!3m2!1sfr!2sfr!4v1552389240290" width="100%" height="380" frameborder="0" style="border:0" allowfullscreen></iframe>
    <div class="container wow fadeInUp mt-5">
        <div class="row justify-content-center">

            <div class="col-lg-3 col-md-4">

                <div class="info">
                    <div>
                        <i class="fa fa-map-marker"></i>
                        <p>242 Rue du Faubourg Saint Antoine<br>Paris, 75012</p>
                    </div>

                    <div>
                        <i class="fa fa-envelope"></i>
                        <p>contact@ffw-pmv.com</p>
                    </div>

                    <div>
                        <i class="fa fa-phone"></i>
                        <p>00 11 22 33 44</p>
                    </div>
                </div>

                <div class="social-links">
                    <a href="#" class="twitter"><i class="fa fa-twitter"></i></a>
                    <a href="#" class="facebook"><i class="fa fa-facebook"></i></a>
                    <a href="#" class="instagram"><i class="fa fa-instagram"></i></a>
                    <a href="#" class="google-plus"><i class="fa fa-google-plus"></i></a>
                    <a href="#" class="linkedin"><i class="fa fa-linkedin"></i></a>
                </div>

            </div>

            <div class="col-lg-5 col-md-8">
                <div class="form">
                    <form action="" method="post" role="form" class="contactForm">
                        <div class="form-group">
                            <input type="text" name="name" class="form-control" id="name" placeholder="{% trans %}Votre nom{% endtrans %}" data-rule="minlen:4" data-msg="Please enter at least 4 chars" />
                            <div class="validation"></div>
                        </div>
                        <div class="form-group">
                            <input type="email" class="form-control" name="email" id="email" placeholder="{% trans %}Votre e-mail{% endtrans %}" data-rule="email" data-msg="Please enter a valid email" />
                            <div class="validation"></div>
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-control" name="subject" id="subject" placeholder="{% trans %}Sujet{% endtrans %}" data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject" />
                            <div class="validation"></div>
                        </div>
                        <div class="form-group">
                            <textarea class="form-control" name="message" rows="5" data-rule="required" data-msg="Please write something for us" placeholder="{% trans %}Message{% endtrans %}"></textarea>
                            <div class="validation"></div>
                        </div>
                        <div class="text-center"><button type="submit">{% trans %}Envoyer{% endtrans %}</button></div>
                    </form>
                </div>
            </div>

        </div>

    </div>
</section><!-- #contact -->
